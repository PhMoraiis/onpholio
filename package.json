{
  "name": "onpholio",
  "module": "src/http/index.ts",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "bun --env-file=.env --watch run src/http/index.ts",
    "start": "bun run src/http/",
    "build": "bun prisma generate && bun build src/http/index.ts",
    "prepare": "husky",
    "commit": "git-cz"
  },
  "prisma": {
    "seed": "bun run src/database/seed.ts"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.3",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/types": "^19.5.0",
    "@types/bun": "latest",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.6"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@fastify/cookie": "^10.0.1",
    "@fastify/cors": "^10.0.1",
    "@fastify/jwt": "^9.0.1",
    "@prisma/client": "^5.20.0",
    "@types/bcrypt": "^5.0.2",
    "bcrypt": "^5.1.1",
    "fastify": "^5.0.0",
    "fastify-type-provider-zod": "^4.0.1",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^5.20.0",
    "zod": "^3.23.8"
  }
}
